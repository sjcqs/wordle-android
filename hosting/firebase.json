{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [ 
      { "source": "wordle/**", "dynamicLinks": true },
      { "source": "!wordle/**", "destination": "/index.html" } 
    ]
  }
}
