import fr.sjcqs.wordle.data.game.entity.Guess;
import java.time.LocalDate;
import kotlin.collections.List;

CREATE TABLE game (
  word TEXT NOT NULL,
  expiredAt TEXT AS LocalDate NOT NULL PRIMARY KEY,
  guesses TEXT AS List<Guess> NOT NULL,
  isFinished INTEGER AS Boolean DEFAULT 0 NOT NULL
);

insertOrReplace:
INSERT OR REPLACE INTO game(word, expiredAt, guesses, isFinished)
VALUES (?, ?, ?, ?);

lastGame:
SELECT * FROM game
ORDER BY expiredAt DESC
LIMIT 1;